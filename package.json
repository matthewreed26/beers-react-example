{
  "name": "jhipster-lite-react-test",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Lite React Test",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build --emptyOutDir",
    "dev": "vite",
    "e2e": "npm run test:component",
    "e2e:headless": "npm run test:component:headless",
    "eslint": "eslint './**/*.{ts,js}' --fix",
    "eslint:ci": "eslint './**/*.{ts,js}'",
    "lint": "eslint --ext .js,.ts,.tsx src/main/webapp/app/**/*",
    "prepare": "husky install",
    "prettier:check": "prettier --check '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "prettier:format": "prettier --write '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:component": "cypress open --config-file src/test/javascript/integration/cypress-config.ts",
    "test:component:headless": "cypress run --headless --config-file src/test/javascript/integration/cypress-config.ts",
    "test:tsc": "tsc --noEmit",
    "test:tsc:watch": "tsc --watch --noEmit",
    "test:watch": "vitest --"
  },
  "dependencies": {
    "@nextui-org/react": "2.2.9",
    "axios": "1.6.7",
    "framer-motion": "11.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.1",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "3.3.1",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.11.19",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-istanbul": "1.2.2",
    "autoprefixer": "10.4.17",
    "cypress": "13.6.4",
    "eslint": "8.56.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-gherkin": "2.2.1",
    "prettier-plugin-java": "2.5.0",
    "prettier-plugin-packagejson": "2.4.11",
    "sass": "1.70.0",
    "tailwindcss": "3.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vite": "5.1.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.2.2",
    "vitest-sonar-reporter": "1.0.0"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
